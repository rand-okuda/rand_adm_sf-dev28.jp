 ShimeKikanMadeShudo__c  +  SeikyuAccount__r.TorihikiSiteNissu__c 
) 
,
IF( SeikyuAccount__r.Kijitsu__c = 99, 

IF(
    MONTH(ShimeKikanMadeNamachi__c)+SeikyuAccount__r.TorihikiSiteTsukisu__c+1&gt;12,
    DATE(YEAR(ShimeKikanMadeNamachi__c)+1,MONTH(ShimeKikanMadeNamachi__c)+SeikyuAccount__r.TorihikiSiteTsukisu__c+1-12,1),
    DATE(YEAR(ShimeKikanMadeNamachi__c)  ,MONTH(ShimeKikanMadeNamachi__c)+SeikyuAccount__r.TorihikiSiteTsukisu__c+1   ,1)
)-1
,
IF(
    MONTH(ShimeKikanMadeNamachi__c)+SeikyuAccount__r.TorihikiSiteTsukisu__c&gt;12,
    DATE(YEAR(ShimeKikanMadeNamachi__c)+1,MONTH(ShimeKikanMadeNamachi__c)+SeikyuAccount__r.TorihikiSiteTsukisu__c-12,SeikyuAccount__r.Kijitsu__c),
    DATE(YEAR(ShimeKikanMadeNamachi__c)  ,MONTH(ShimeKikanMadeNamachi__c)+SeikyuAccount__r.TorihikiSiteTsukisu__c   ,SeikyuAccount__r.Kijitsu__c)
)

)


)</formula>
